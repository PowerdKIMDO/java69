package game.baseball;

public class Test1 {
	int g_i;
	
	void methodA() {
		int a_i;
		a_i = 1;
		g_i++;
	}
	void methodB() {
		g_i = g_i+5;
	}
	public static void main(String[] args) {
		//insert here -  methodA를 호출 하였다.
		//g_i는 얼마일까요?
		Test1 t1 = new Test1();
		t1.methodA();
		System.out.println(t1.g_i);
	}

}
