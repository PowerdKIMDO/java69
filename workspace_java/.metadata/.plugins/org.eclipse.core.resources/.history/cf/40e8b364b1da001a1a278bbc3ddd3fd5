package book.ch4;

public class SwitchTest1 {
	public int others(int x) {
		
		return x;
	}
	public static int switchit(int x) {
		int j = 1;
		switch(x) {
		case 1: j++;
		case 2: j++;
		case 3: j++;
		case 4: j++;
		case 5: j++;
		default: j++;
		}
		return j+x;
	}
	public static void main(String[] args) {
		//insert here - switchit 호출시 x값을 4로 한다면 출력 결과는 얼마일까요?


	}

}
