package book.ch4;

public class SwitchTest2 {

	public static void main(String[] args) {
		int i = 0;
		boolean isOk = false;
		while(!isOk) {
			String msg = "오늘 스터디 할까?";
			switch(i) {
			case 0:{
				String msg = "";
			}break;
			default:
				break;
			}
		}
	}////////////////end of main
}////////////////////end of SwitchTest2
