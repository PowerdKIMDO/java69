package book.ch4;

public class SwitchTest1 {
	public int others(int x) {//x=5 일때를 기준으로 말해 본다.
		switch(x) {
			case 6: x--;
			case 5: x--;
			case 4: x--;
			case 3: x--;
			break;
			default: x--;
			break;
		}////////////////end of switch	
		return x;
	}
	public static int switchit(int x) {
		int j = 1;
		switch(x) {
			case 1: j++;
			case 2: j++;
			case 3: j++;
			case 4: j++;
			case 5: j++;
			default: j++;
		}////////////////end of switch
		return j+x;
	}
	public static void main(String[] args) {
		//insert here - switchit 호출시 x값을 4로 한다면 출력 결과는 얼마일까요?


	}

}
