package game.baseball;

public class BBGameSimulation {

	public static void main(String[] args) {
		BaseBallGame bbg = new BaseBallGame();
		bbg.ranCom();
		System.out.println(bbg.com[0]+""+bbg.com[1]+""+bbg.com[2]);
		//만일 10번 확인하고 싶으면
		for(int i=0;i<10;i++) {
			bbg.ranCom();
			System.out.println(bbg.com[0]+""+bbg.com[1]+""+bbg.com[2]);
		}
	}

}
